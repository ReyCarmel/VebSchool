<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.venture.vebschool">

    <application
        android:allowBackup="true"
        android:icon="@drawable/veb_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MenuActivities.PaymentActivity"></activity>
        <activity android:name=".MenuActivities.NewsActivity" />
        <activity android:name=".MenuActivities.EventActivity" />
        <activity android:name=".MenuActivities.ExamActivity" />
        <activity android:name=".MenuActivities.MessageActivity" />
        <activity android:name=".MenuActivities.TimetableActivity" />
        <activity android:name=".MenuActivities.MarkActivity" />
        <activity android:name=".MenuActivities.AttendanceActivity" />
        <activity android:name=".MenuActivities.StudentActivity" />
        <activity android:name=".Submenu.LogoutActivity" />
        <activity android:name=".Submenu.SettingsActivity" />
        <activity android:name=".Submenu.ProfileActivity" />
        <activity
            android:name=".Login"
            android:label="Login"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivitySplashScreen"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Home"
            android:label="Home" />
    </application>

</manifest>